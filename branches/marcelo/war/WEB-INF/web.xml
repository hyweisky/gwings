<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
  
  <!-- Servlets -->
	<servlet>
		<servlet-name>remoteLineProvider</servlet-name>
		<servlet-class> 
		      org.gwings.server.demo.pagination.RemoteLineProvider
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>remoteLoggingServiceImpl
		</servlet-name>
		<servlet-class>
			 com.google.gwt.libideas.logging.server.RemoteLoggingServiceImpl
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>remoteLoggingServiceImpl</servlet-name>
		<url-pattern>/gwingsdemo/logging</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>remoteLineProvider</servlet-name>
		<url-pattern>/gwingsdemo/provider</url-pattern>
	</servlet-mapping>

        <!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>GWingsDemo.html</welcome-file>
	</welcome-file-list>
</web-app>